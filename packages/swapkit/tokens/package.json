{
  "author": "swapkit-oss",
  "description": "SwapKit Lib tokens",
  "devDependencies": {
    "@swapkit/api": "workspace:*",
    "@swapkit/types": "workspace:*",
    "@types/fs-extra": "11.0.4",
    "@vitest/coverage-istanbul": "1.3.1",
    "fs-extra": "11.2.0",
    "vite": "5.1.3",
    "vitest": "1.3.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "homepage": "https://github.com/thorswap/SwapKit",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "name": "@swapkit/tokens",
  "react-native": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thorswap/SwapKit.git"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist vite.config.ts.* node_modules",
    "generate-tokens": "node ./scripts/fetchTokenLists.js",
    "lint": "biome check --apply ./src",
    "test": "echo 'vitest --run'",
    "test:coverage": "echo 'vitest run --coverage'",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "1.0.0-rc.35"
}
